#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ADMIN/Desktop/Teach2Give/Week5/bug-tracker2/node_modules/.pnpm/mssql@12.0.0/node_modules/mssql/bin/node_modules:/mnt/c/Users/ADMIN/Desktop/Teach2Give/Week5/bug-tracker2/node_modules/.pnpm/mssql@12.0.0/node_modules/mssql/node_modules:/mnt/c/Users/ADMIN/Desktop/Teach2Give/Week5/bug-tracker2/node_modules/.pnpm/mssql@12.0.0/node_modules:/mnt/c/Users/ADMIN/Desktop/Teach2Give/Week5/bug-tracker2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ADMIN/Desktop/Teach2Give/Week5/bug-tracker2/node_modules/.pnpm/mssql@12.0.0/node_modules/mssql/bin/node_modules:/mnt/c/Users/ADMIN/Desktop/Teach2Give/Week5/bug-tracker2/node_modules/.pnpm/mssql@12.0.0/node_modules/mssql/node_modules:/mnt/c/Users/ADMIN/Desktop/Teach2Give/Week5/bug-tracker2/node_modules/.pnpm/mssql@12.0.0/node_modules:/mnt/c/Users/ADMIN/Desktop/Teach2Give/Week5/bug-tracker2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/mssql" "$@"
else
  exec node  "$basedir/../../bin/mssql" "$@"
fi
